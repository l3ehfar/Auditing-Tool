{
  "name": "auditing",
  "description": "A Marcelle Application with SvelteKit",
  "version": "0.0.0",
  "keywords": [
    "marcelle"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "backend": "marcelle-backend"
  },
  "dependencies": {
    "@dgmjs/core": "^0.31.2",
    "@feathersjs/authentication": "^5.0.31",
    "@feathersjs/authentication-client": "^5.0.31",
    "@feathersjs/authentication-local": "^5.0.31",
    "@feathersjs/feathers": "^5.0.31",
    "@feathersjs/rest-client": "^5.0.31",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/svelte-fontawesome": "^0.2.2",
    "@huggingface/inference": "^2.8.0",
    "@marcellejs/backend": "^0.6.5",
    "@marcellejs/core": "^0.6.5",
    "@xenova/transformers": "^2.17.2",
    "chart.js": "^4.4.4",
    "chartjs-chart-matrix": "^2.0.1",
    "fabric": "^6.4.2",
    "overlayscrollbars": "^2.10.0",
    "scule": "^1.3.0"
